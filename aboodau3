local Players = game:GetService("Players")
local plr = Players.LocalPlayer

-- [[ ŸÜÿ∏ÿßŸÖ ÿßŸÑŸÄ BLACKLIST ]]
local BLACKLIST = {  
    "Waleedking70", "JJFH23456771", "omarbkp", 
    "fastma_storage", "No_Louay", "mlk123456771", "fling_2009", "fahad_moen3", "CmdBarDestroyer"
}  

local function IsPlayerBlacklisted(playerName)  
    local nameLower = playerName:lower()  
    for _, blacklistedName in ipairs(BLACKLIST) do  
        if nameLower == blacklistedName:lower() then return true end  
    end  
    return false 
end  

if IsPlayerBlacklisted(plr.Name) then  
    local warningGui = Instance.new("ScreenGui", plr.PlayerGui) 
    local warningFrame = Instance.new("Frame", warningGui)  
    warningFrame.Size = UDim2.new(0, 400, 0, 200)  
    warningFrame.Position = UDim2.new(0.5, -200, 0.5, -100)  
    warningFrame.BackgroundColor3 = Color3.fromRGB(20, 0, 0)  
    Instance.new("UICorner", warningFrame).CornerRadius = UDim.new(0, 15)
    local title = Instance.new("TextLabel", warningFrame)  
    title.Size = UDim2.new(1, 0, 0, 50)  
    title.Text = "‚ö†Ô∏è BLACKLIST DETECTED ‚ö†Ô∏è"  
    title.TextColor3 = Color3.new(1, 1, 1)  
    title.Font = Enum.Font.GothamBlack 
    title.TextScaled = true  
    task.wait(3)  
    plr:Kick("üö´ ÿØÿ≤")
    return 
end

