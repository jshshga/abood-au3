MainStroke.Thickness = 3
MainStroke.Color = Color3.fromRGB(0, 255, 255)

-- [[ ÿ£ŸÜŸÖŸäÿ¥ŸÜ ÿßŸÑÿ±ŸäŸÜÿ®Ÿà ÿπŸÑŸâ ÿ¨ŸàÿßŸÜÿ® ÿßŸÑÿ≥ŸÉÿ±ÿ®ÿ™ ]]
local function BlastEffect()
    task.spawn(function()
        local rainbowColors = {
            Color3.new(1, 0, 0), Color3.new(1, 0.5, 0), Color3.new(1, 1, 0),
            Color3.new(0, 1, 0), Color3.new(0, 0, 1), Color3.new(0.5, 0, 1)
        }
        for i = 1, 2 do
            for _, color in ipairs(rainbowColors) do
                TweenService:Create(MainStroke, TweenInfo.new(0.1), {Color = color}):Play()
                task.wait(0.05)
            end
        end
        TweenService:Create(MainStroke, TweenInfo.new(0.2), {Color = Color3.fromRGB(0, 255, 255)}):Play()
    end)
end

local currentPrefix = ";" 
local attackCommands = { "logs [target]", "re [target]" } 

local function SaveCommands()
    pcall(function() writefile("ABOAU_Config.json", HttpService:JSONEncode(attackCommands)) end)
end

local function LoadCommands()
    pcall(function()
        if isfile and isfile("ABOAU_Config.json") then
            attackCommands = HttpService:JSONDecode(readfile("ABOAU_Config.json"))
        end
    end)
end
LoadCommands()

local function ClickEffect(button)
    local originalSize = button.Size
    local tweenInfo = TweenInfo.new(0.05, Enum.EasingStyle.Quart, Enum.EasingDirection.Out, 0, true)
    local tween = TweenService:Create(button, tweenInfo, {
        Size = UDim2.new(originalSize.X.Scale * 1.05, originalSize.X.Offset + 5, originalSize.Y.Scale * 1.05, originalSize.Y.Offset + 5)
    })
    tween:Play()
end

