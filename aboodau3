local antiSpamActive = false
local deleteTargets = {
    PlayerGui = {"CommandMenucommandLogs", "CommandMenuchatLogs", "Notices", "CustomTopBar", "CmdBar"},
    Workspace = {"HDAdminNightVision"},
    Characters = {"HDAdminNightVision"}
}

local function toggleLOR_Assets(state)
    pcall(function()
        local assets = ReplicatedStorage:WaitForChild("HDAdminHDClient"):WaitForChild("Signals").Parent:WaitForChild("Assets")
        local nv = assets:FindFirstChild(state == "start" and "NightVision" or "1NightVision")
        if nv then nv.Name = (state == "start" and "1NightVision" or "NightVision") end
        local templates = plr.PlayerGui:WaitForChild("HDAdminInterface"):WaitForChild("MenuTemplates")
        local temp = templates:FindFirstChild(state == "start" and "Template5" or "Template")
        if temp then temp.Name = (state == "start" and "Template" or "Template5") end
    end)
end

RunService.Heartbeat:Connect(function()
    if antiSpamActive then
        pcall(function()
            for _, name in ipairs(deleteTargets.PlayerGui) do
                local t = plr.PlayerGui:FindFirstChild(name, true) if t then t:Destroy() end
            end
            for _, v in ipairs(plr.PlayerGui:GetDescendants()) do
                if v.Name:find("Notice") or v.Name:find("Logs") or v.Name:find("Alert") then v:Destroy() end
            end
        end)
    end
end)

