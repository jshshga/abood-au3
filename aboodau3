local AntiSpamBtn = CreateSetBtn("ANTI SPAM: OFF ‚ùå", Color3.fromRGB(255, 100, 0))

local AddCmdFrame = Instance.new("Frame", SettingsScroll)
AddCmdFrame.Size = UDim2.new(0.9, 0, 0, 45)
